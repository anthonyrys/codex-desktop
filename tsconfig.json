{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "dom",
      "es2015",
      "es2016",
      "es2017"
    ],
    "allowJs": true,
    "jsx": "react-jsx",
    "sourceMap": true,
    "outDir": "./dist",
    "strict": true ,
    "esModuleInterop": true,
    "baseUrl": "./",
    "paths": {
        "@main/*": ["src/main/*"],
        "@renderer/*": ["src/renderer/*"],
        
        "@components": ["src/renderer/components"],
        "@styles/*": ["src/renderer/styles/*"],
        "@ui": ["src/renderer/ui"],
        "@utils/*": ["src/renderer/utils/*"]
    },
  }
}
